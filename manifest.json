{
    "manifest_version": 3,
    "name": "ImageReaper",
    "version": "0.1.0",
    "description": "Scan pages for images from specified hosts and download them (development build).",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "options_page": "options.html"
}